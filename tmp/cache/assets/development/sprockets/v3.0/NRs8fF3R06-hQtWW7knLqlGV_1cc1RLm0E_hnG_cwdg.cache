(function() {
  this.AboutItem = React.createClass({
    getInitialState: function() {
      return {
        title: this.props.title,
        description: this.props.description,
        order: this.props.order
      };
    },
    rawMarkup: function() {
      return {
        __html: marked(this.state.description, {
          sanitize: true
        })
      };
    },
    render: function() {
      return <li>
      <p>
        <span this.state.title />
        <span dangerouslySetInnerHTML={{__html: this.state.description}} />
      </p>
    </li>;
    }
  });

}).call(this);
