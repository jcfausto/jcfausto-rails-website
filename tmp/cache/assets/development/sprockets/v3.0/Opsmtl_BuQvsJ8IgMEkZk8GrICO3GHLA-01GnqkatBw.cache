(function() {
  this.PhraseShuffler = React.createClass({
    getInitialState: function() {
      return {
        phrases: this.props.phrases
      };
    },
    getDefaultProps: function() {
      return {
        current_phrase: "Shuffler"
      };
    },
    shuffle: function() {
      return this.state.current_phrase = "Current shuffle";
    },
    render: function() {
      return <h1>Phrase Shuffler - {this.state.current_phrase}</h1>;
    }
  });

}).call(this);
